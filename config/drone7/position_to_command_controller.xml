<Pos2Cmd_Controller>
        
	<PID_X2Pitch>
		
		<Gain1> <!-- First gains for steps around 0.25 meter -->
			<Kp> 0.1000	    </Kp>  <!-- <Kp> 0.065     </Kp> Proportional Gain -->
			<Kd> 0.0100     </Kd>  <!-- <Kd> 0.045     </Kd> Derivative Gain   --> 
			<Ki> 0.0003     </Ki>  <!-- <Ki> 0.005     </Ki> Integral Gain     -->
		</Gain1>

		<Gain2> <!-- Second gains for steps around 5 meter -->
			<Kp> 0.025     </Kp>  <!-- <Kp> 0.065     </Kp> Proportional Gain -->
			<Kd> 0.010     </Kd>  <!-- <Kd> 0.045     </Kd> Derivative Gain   --> 
			<Ki> 0.000     </Ki>  <!-- <Ki> 0.005     </Ki> Integral Gain     -->
		</Gain2>

		<Saturation> <!-- Control Input Saturation -->
			<enable_saturation> 1.0 </enable_saturation> <!-- 1.0 means saturation enabled, 0.0 disabled -->
			<SatMax>  0.25 </SatMax>  <!-- Max Saturation Value -->
			<SatMin> -0.25 </SatMin>  <!-- Min Saturation Value -->
		</Saturation>
		
		<Anti_wind_up>
			<enable_anti_wind_up> 1.0 </enable_anti_wind_up> <!-- 1.0 means anti-WindUp enabled, 0.0 disabled -->
			<Kw> 0.30		</Kw>  <!-- <Kw> 0.50   </Kw> -->
		</Anti_wind_up>

	</PID_X2Pitch>

	<PID_Y2Roll>	
	
		<Gain1> <!-- First gains for steps around 0.25 meter -->
			<Kp> 0.1000		</Kp>  <!--  Proportional Gain -->
			<Kd> 0.0100		</Kd>  <!--  Derivative Gain   --> 
			<Ki> 0.0003		</Ki>  <!--  Integral Gain     -->
		</Gain1>

		<Gain2> <!-- Second gains for steps around 5 meter -->
			<Kp> 0.025     </Kp>  <!-- <Kp> 0.065     </Kp> Proportional Gain -->
			<Kd> 0.010     </Kd>  <!-- <Kd> 0.045     </Kd> Derivative Gain   --> 
			<Ki> 0.000     </Ki>  <!-- <Ki> 0.005     </Ki> Integral Gain     -->
		</Gain2>

		<Saturation> <!-- Control Input Saturation -->
			<enable_saturation> 1.0 </enable_saturation> <!-- 1.0 means saturation enabled, 0.0 disabled -->
			<SatMax>  0.25 </SatMax>  <!-- Max Saturation Value -->
			<SatMin> -0.25 </SatMin>  <!-- Min Saturation Value -->
		</Saturation>
		
		<Anti_wind_up>
			<enable_anti_wind_up> 1.0 </enable_anti_wind_up> <!-- 1.0 means anti-WindUp enabled, 0.0 disabled -->
			<Kw> 0.30		</Kw>  <!-- <Kw> 0.50   </Kw> -->
		</Anti_wind_up>

	</PID_Y2Roll>

</Pos2Cmd_Controller>
