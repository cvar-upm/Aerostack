<Trajectory_Controller>

	<Constants>
		<Vmaxxy> 	0.75	</Vmaxxy>
		<Vmaxz> 	0.40	</Vmaxz>
		<t_90> 		5.00 	</t_90>
	</Constants>
        
	<PID_Pitch>	
	
		<Gain>
			<Kp> 0.30		</Kp>  <!--  Proportional Gain -->
			<Kd> 0.03		</Kd>  <!--  Derivative Gain   --> 
			<Ki> 0.10		</Ki>  <!--  Integral Gain     -->
		</Gain>

		<Saturation> <!-- Control Input Saturation -->
			<enable_saturation> 1.0 </enable_saturation> <!-- 1.0 means saturation enabled, 0.0 disabled -->
			<SatMax>  0.50	</SatMax>  <!-- Max Saturation Value -->
			<SatMin> -0.50	</SatMin>  <!-- Min Saturation Value -->
		</Saturation>
		
		<Anti_wind_up>
			<enable_anti_wind_up> 0.0 </enable_anti_wind_up> <!-- 1.0 means anti-WindUp enabled, 0.0 disabled -->
			<Kw> 			0.60	</Kw>  <!-- <Kw> 0.50   </Kw> -->
		</Anti_wind_up>

	</PID_Pitch>

	<PID_Roll>	
	
