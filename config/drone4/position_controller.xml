<Position_Controller>
        
	<PID_X2Dx>
		
		<Gain>
			<Kp> 1.00     </Kp>  <!-- <Kp> 1.75     </Kp> Proportional Gain -->
			<Kd> 0.20     </Kd>  <!-- <Kd> 0.90     </Kd> Derivative Gain   --> 
			<Ki> 0.00     </Ki>  <!-- <Ki> 0.10     </Ki> Integral Gain     -->
		</Gain>

		<Saturation> <!-- Control Input Saturation -->
			<enable_saturation> 1.0 </enable_saturation> <!-- 1.0 means saturation enabled, 0.0 disabled -->
			<SatMax>  1.0 </SatMax>  <!-- Max Saturation Value -->
			<SatMin> -1.0 </SatMin>  <!-- Min Saturation Value -->
		</Saturation>
		
		<Anti_wind_up>
			<enable_anti_wind_up> 1.0 </enable_anti_wind_up> <!-- 1.0 means anti-WindUp enabled, 0.0 disabled -->
			<AwuMax>  0.4 </AwuMax>  <!-- Max A-WU Value -->
			<AwuMin> -0.4 </AwuMin>  <!-- Min A-WU Value -->
			<Kw> 0.50		</Kw>  <!-- <Kw> 0.50   </Kw> -->
		</Anti_wind_up>

	</PID_X2Dx>

	<PID_Y2Dy>	
	
		<Gain>
			<Kp> 1.00		</Kp>  <!--  Proportional Gain -->
			<Kd> 0.20		</Kd>  <!--  Derivative Gain   --> 
			<Ki> 0.00   </Ki>  <!--  Integral Gain     -->
		</Gain>

		<Saturation> <!-- Control Input Saturation -->
			<enable_saturation> 1.0 </enable_saturation> <!-- 1.0 means saturation enabled, 0.0 disabled -->
			<SatMax>  1.0 </SatMax>  <!-- Max Saturation Value -->
			<SatMin> -1.0 </SatMin>  <!-- Min Saturation Value -->
		</Saturation>
		
		<Anti_wind_up>
			<enable_anti_wind_up> 1.0 </enable_anti_wind_up> <!-- 1.0 means anti-WindUp enabled, 0.0 disabled -->
			<AwuMax>  0.4 </AwuMax>  <!-- Max A-WU Value -->
			<AwuMin> -0.4 </AwuMin>  <!-- Min A-WU Value -->
			<Kw> 0.50		</Kw>  <!-- <Kw> 0.50   </Kw> -->
		</Anti_wind_up>

	</PID_Y2Dy>

</Position_Controller>
